{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.20.0",
    "jest": "^24.9.0",
    "nodemon": "^2.0.2",
    "sucrase": "^3.12.1"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add",
      "git commit -m \"chore: autofixed code by lint-staged\""
    ]
  },
  "scripts": {
    "dev": "nodemon src/server.js --ignore __tests__",
    "debug": "node --inspect-brk -r sucrase/register src/server.js",
    "test": "jest",
    "lint": "eslint \"**/*.js\" --ignore-pattern node_modules/ --ignore-pattern dist/",
    "start": "node -r sucrase/register src/server.js"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "eslint-import-resolver-node": "^0.3.3",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "lint-staged": "^10.0.1",
    "mongoose": "^5.8.9",
    "youch": "^2.0.10"
  }
}
